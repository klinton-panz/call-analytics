# Copy this to .env and fill in your values

# Server Configuration
PORT=8080
NODE_ENV=development

# Security
SECRET=MyStrongSecretKey123!

# Database - Replace with your actual Supabase/PostgreSQL connection string
DATABASE_URL=postgresql://postgres:your_password@your_host:6543/postgres?sslmode=require

# Optional: Caching settings
POLL_INTERVAL_MS=30000
CACHE_TTL_MS=300000